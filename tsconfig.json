{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    // "baseUrl": "src",
    "strict": false,
    "strictPropertyInitialization": false,
    "allowJs": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./src/main/config/@types",
      "./node_modules/@types"
    ]
  }
}